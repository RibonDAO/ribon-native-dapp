{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "allowJs": true,
    "sourceMap": true,
    "baseUrl": "src",
    "sourceRoot": "/",
    "target": "es2015",
    "jsx": "react",
    "noEmit": true,
    "moduleResolution": "node",
    "paths": {
      "src/*": ["./*"],
    }
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules",
  ]
}
